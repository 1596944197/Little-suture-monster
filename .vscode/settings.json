{
  "eslint.validate": [
    "javascript",
    "typescript",
    "typescriptreact",
  ],
  "files.eol": "\n",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "path-intellisense.mappings": {
    "@":"${workspaceRoot}/renderer"
  },
  "cssModules.pathAlias": {
    "@": "${workspaceFolder}/renderer",
  }
}